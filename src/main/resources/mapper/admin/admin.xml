<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.pcwk.ehr.admin">

	<insert id="doSaveCategory" parameterType="CategoryVO">
		INSERT INTO category (
			CATEGORY_NO,
			CATEGORY_NM
		) VALUES (
			#{categoryNo},
			#{categoryNm}
		)
	</insert>
	
  <!-- mybatis-config.xml에 alias 설정했기때문에 parameterType 풀경로X -->
  <delete id="doDelete" parameterType="UserVO">  
       DELETE FROM hr_member
       WHERE u_id = #{uId}
  </delete>  <!-- #{uId} 는 UserVO의 멤버변수명 -->

</mapper>